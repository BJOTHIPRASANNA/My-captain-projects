import java.util.*;
public class Switch_code {
    public static void main(String args[])
    {
        int choice;
        System.out.println("Enter your option 1.hi\t2.say hello\t3.say welcome");
        Scanner sc = new Scanner(System.in);
        choice=sc.nextInt();
        switch(choice)
        {
            case 1:
                System.out.println("you said hi");
                break;
            case 2:
                System.out.println("you said hello");
                break;
            case 3:
                System.out.println("you said welocome");
                break;
            default:
                System.out.println("Enter a valid number");
        }
        
    }
}
